{
    "name": "github-ai-agent",
    "displayName": "GitHub AI Agent",
    "description": "GitHub Copilot alternative with PR analysis, code completion, and test generation",
    "version": "1.0.0",
    "publisher": "github-ai",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "AI",
        "Code Completion",
        "Testing",
        "Other"
    ],
    "keywords": [
        "copilot",
        "ai",
        "code-completion",
        "test-generation",
        "pr-analysis",
        "github"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "github-ai-agent.completeCode",
                "title": "GitHub AI: Complete Code",
                "category": "AI Agent"
            },
            {
                "command": "github-ai-agent.generateTests",
                "title": "GitHub AI: Generate Tests",
                "category": "AI Agent"
            },
            {
                "command": "github-ai-agent.analyzePR",
                "title": "GitHub AI: Analyze PR",
                "category": "AI Agent"
            },
            {
                "command": "github-ai-agent.showPanel",
                "title": "GitHub AI: Show Agent Panel",
                "category": "AI Agent"
            },
            {
                "command": "github-ai-agent.openSettings",
                "title": "GitHub AI: Open Settings",
                "category": "AI Agent"
            },
            {
                "command": "github-ai-agent.toggleAutoComplete",
                "title": "GitHub AI: Toggle Auto-Complete",
                "category": "AI Agent"
            },
            {
                "command": "github-ai-agent.toggleAutoAnalyze",
                "title": "GitHub AI: Toggle Auto-Analyze PR",
                "category": "AI Agent"
            }
        ],
        "keybindings": [
            {
                "command": "github-ai-agent.completeCode",
                "key": "ctrl+shift+space",
                "mac": "cmd+shift+space",
                "when": "editorTextFocus"
            },
            {
                "command": "github-ai-agent.generateTests",
                "key": "ctrl+shift+t",
                "mac": "cmd+shift+t",
                "when": "editorTextFocus"
            },
            {
                "command": "github-ai-agent.showPanel",
                "key": "ctrl+shift+g",
                "mac": "cmd+shift+g"
            }
        ],
        "views": {
            "explorer": [
                {
                    "id": "github-ai-agent-panel",
                    "name": "GitHub AI Agent",
                    "icon": "resources/icon.svg",
                    "contextualTitle": "AI Agent"
                }
            ]
        },
        "configuration": {
            "title": "GitHub AI Agent",
            "properties": {
                "github-ai-agent.apiEndpoint": {
                    "type": "string",
                    "default": "http://localhost:5000",
                    "description": "API endpoint for GitHub AI Agent backend"
                },
                "github-ai-agent.enableAutoComplete": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enable automatic code completion suggestions"
                },
                "github-ai-agent.enableAutoAnalyze": {
                    "type": "boolean",
                    "default": false,
                    "description": "Enable automatic PR analysis on file save"
                },
                "github-ai-agent.completionDelay": {
                    "type": "number",
                    "default": 1000,
                    "description": "Delay in milliseconds before showing completion suggestions",
                    "minimum": 100,
                    "maximum": 5000
                },
                "github-ai-agent.testFramework": {
                    "type": "string",
                    "enum": [
                        "pytest",
                        "unittest",
                        "jest",
                        "vitest",
                        "mocha"
                    ],
                    "default": "pytest",
                    "description": "Default test framework for test generation"
                },
                "github-ai-agent.coverageTarget": {
                    "type": "number",
                    "default": 80,
                    "description": "Target code coverage percentage",
                    "minimum": 0,
                    "maximum": 100
                },
                "github-ai-agent.maxSuggestions": {
                    "type": "number",
                    "default": 5,
                    "description": "Maximum number of completion suggestions",
                    "minimum": 1,
                    "maximum": 10
                }
            }
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "github-ai-explorer",
                    "title": "GitHub AI Agent",
                    "icon": "resources/icon.svg"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run esbuild-base -- --minify",
        "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --platform=node",
        "esbuild": "npm run esbuild-base -- --sourcemap",
        "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
        "test": "node ./out/test/runTest.js",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "lint": "eslint src --ext ts"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "@types/vscode": "^1.85.0",
        "@typescript-eslint/eslint-plugin": "^6.13.0",
        "@typescript-eslint/parser": "^6.13.0",
        "esbuild": "^0.19.10",
        "eslint": "^8.54.0",
        "typescript": "^5.3.2"
    },
    "dependencies": {
        "axios": "^1.6.2"
    }
}